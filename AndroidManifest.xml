<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.domotic.enhanced"
  android:versionCode="1"
  android:versionName="0.1" >

  <uses-sdk
    android:minSdkVersion="16"
    android:targetSdkVersion="23" />

  <uses-permission android:name="android.permission.INTERNET" />

  <application
    android:allowBackup="true"
    android:icon="@drawable/ic_launcher"
    android:label="@string/app_name"
    android:theme="@style/AppTheme" >
    <activity android:name="com.domotic.enhanced.activity.MainActivity_" >
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:name="com.domotic.enhanced.activity.EditLightActivity_"
      android:label="@string/activity_light"
      android:parentActivityName="com.domotic.enhanced.activity.MainActivity_" />
    <activity
      android:name="com.domotic.enhanced.activity.EditIpCamActivity_"
      android:label="@string/activity_ipcam"
      android:parentActivityName="com.domotic.enhanced.activity.MainActivity_" />
    <activity
      android:name="com.domotic.enhanced.activity.IpCamFullActivity_"
      android:label="@string/activity_ipcam"
      android:parentActivityName="com.domotic.enhanced.activity.MainActivity_" />
  </application>

</manifest>